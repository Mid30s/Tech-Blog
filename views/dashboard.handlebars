<div class="container">
    <div class="row">
        <div class="col-md-12">
            <h2 class="mt-4 mb-4">Dashboard</h2>
            <a href="/new-post" class="btn btn-primary mb-4">New Post</a>
            {{#each posts}}
                {{> post}}
            {{else}}
                <p>No posts available</p>
            {{/each}}
        </div>
    </div>
</div>
